<services>
    <service name="obs_scm">
        <param name="scm">git</param>
        <param name="url">https://github.com/luc14n0/openSUSE-kernel.git</param>
        <param name="revision">refs/heads/linux-rolling-stable</param>
        <param name="filename">linux</param>
        <param name="versionformat">@PARENT_TAG@</param>
        <param name="versionrewrite-pattern">v?([0-9.]+)(\+0)?</param>
        <param name="versionrewrite-replacement">\1</param>
    </service>
    <service name="tar" mode="buildtime"/>
</services>
